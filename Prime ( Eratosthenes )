
bool ok[MAXN];
vi prime;

void eratosthenes(int N, bool ok)
{
    for(int i = 4 ; i <= N ; i += 2)
        ok[i] = 1;
    for(int i = 3 ; i * i <= N ; i += 2)
        if(!ok[i])
            for(int j = i * i ; j <= N ; j += i)
                ok[j] = 1;
    if(ok == 0)
        return;
    prime.eb(2);
    for(int i = 3 ; i <= N ; i += 2)
        if(!ok[i]) prime.eb(i);
}
